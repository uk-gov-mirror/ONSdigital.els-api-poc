{
  "name": "@onsvisual/sveltekit-starter",
  "homepage": "http://onsvisual.github.io/sveltekit-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 vite dev",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:preview": "cross-env PUBLIC_APP_ENV=preview vite build",
    "package": "vite package",
    "preview": "vite preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "make-json-stat": "node ./scripts/make-json-stat.js",
    "test-arquero": "npx ts-node ./scripts/arquero-processing.ts",
    "make-geo-data": "node ./scripts/make-geo-data.js",
    "make-taxonomy": "node ./scripts/make-taxonomy.js",
    "make-clusters": "node ./scripts/make-clusters.js",
    "data:download": "tsx ./scripts/download-data.ts"
  },
  "author": "Ahmad Barclay",
  "license": "MIT",
  "devDependencies": {
    "@mapbox/tilebelt": "^2.0.2",
    "@onsvisual/robo-utils": "^0.3.7",
    "@onsvisual/svelte-charts": "^0.4.11",
    "@onsvisual/svelte-components": "^1.0.12",
    "@sveltejs/adapter-netlify": "^5.2.4",
    "@sveltejs/adapter-node": "^5.3.3",
    "@sveltejs/kit": "^2.46.5",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/meta": "^7.2.0",
    "arquero": "^8.0.3",
    "cross-env": "^7.0.3",
    "d3-dsv": "^3.0.1",
    "d3-format": "^3.1.0",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "svelteplot": "^0.3.7",
    "throttleit": "^2.1.0",
    "topojson-client": "^3.1.0",
    "topojson-server": "^3.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vite": "^5.4.4"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  },
  "type": "module"
}
